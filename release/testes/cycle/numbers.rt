
# get random spaces
getSpaces(min: UInt, max: UInt) -> String
  l = 0
  result = ""
  length = randUInt(min,max)
  ? l < length
    l += 1
    result += " "
    go(1)
  = result

# output random numbers from 0 to a, 
# and indents from 1 to a
cycle
  sleep(a/2)
  a = 0
  ? a < 32
    print(f"{randUInt(0,a)}{getSpaces(1,a)}")
    a += 1
    go(1)

# main loop for 1000 times
b = 0
? b < 1000
  cycle()
  b += 1
  go(1)