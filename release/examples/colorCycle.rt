
# Данный пример выводит подряд кучу цветных квадратов с номерами;
  Здесь есть: 
    Ссылки на структуры;
    Методы;
    Случайность randUInt;
    Приведение типов;
    Условия и циклы.

# get hex char
getHexChar -> String
  rand = randUInt(0,15)
  ? rand = 10
    rand = 'a'
  ? rand = 11
    rand = 'b'
  ? rand = 12
    rand = 'c'
  ? rand = 13
    rand = 'd'
  ? rand = 14
    rand = 'e'
  ? rand = 15
    rand = 'f'
  = String(rand)

# get random string of hex color
getHex -> String
  result = "#" # hex color
  l = 0        # color cycle
  ? l < 6
    result += getHexChar()
    l += 1
    go(1)
  = result

# get random spaces
getSpaces -> String
  l = 0
  result = ""
  ? l < randUInt(0,5)
    l += 1
    result += " "
    go(1)
  = result

# displays stripes of different colors with numbers
write
  i = 0 # 0-9 counter
  c = 0 # main cycles counter
  ? i < 10
    # print and go next char
    print(f"\bg({getHex()})\fg({getHex()}){c}{getSpaces()}\c{getSpaces()}")
    i += 1
    ? i >= 10
      ? c < 32
        i = 0
        c += 1
    go(1)
  println("\c")

# main loop for 10 times
j = 0
? j < 10
  write()
  j += 1
  go(1)
