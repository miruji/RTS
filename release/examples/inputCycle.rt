
# Это пример работы со статическими ссылками,
  а также input, randUInt;

a
 10
 20
 30
 62

println(f"{a}:{type(a)}:{len(a)}")
a0 = String(a.0)
println(f"{a.0}:{type(a.0)}:{a0}:{type(a0)}")
a1 = UInt(a0)
println(f"{a1}:{type(a1)}")

println(f"{a.3}:{Char(a.3)}")


a2 = input("input please: ")
println(a2)

l = 0
? l < UInt(a2)
  print(f"{l}: ")
  println(f"{randUInt(0,10)}")
  l += 1
  go(1)
?
  println("else")
