
# Объявляем вложенные структуры ссылающиеся по цепочке
  и хотим проверить статические ссылки
a
  10
  20

b
  a
  10

c() -> UInt
  b
  10
  println(111)
  = 123

# Проверяем результаты статических ссылок
res = 0

? String(b.0) != "a"
  res = 1
? b.0.0 != 10
  res = 2
? c.0.0.0 != 10
  res = 3
? c.b.0.0 != 10
  res = 4
? c.b.a.0 != 10
  res = 5

# Дополнительные проверки
d = 100
? d != 100
  res = 6

? c() != 123
  res = 7

e
  f() -> UInt
    1
    println(10)
    = 10

? e.f() != 10
  res = 8

# Если не было ошибок при проверке, то всё правильно
exit(res)