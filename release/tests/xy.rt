
xy(x: UInt, y: Int) -> String
  # Выводим x, y и их типы данных
  print(f"{x}:{type(x)} - {y}:{type(y)} ")
  # Приводим x к Int для получения общего Int результата,
  = String(Int(x)-y) # а потом получаем String из Int

result = xy(25, 30) # Это будет строка
println(f"= {result}:{type(result)}") # -5

# Для правильной проверки приводим result к Int
exit(Int(result) != -5) # Если result = -5 значит всё верно