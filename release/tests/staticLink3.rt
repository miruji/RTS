
# Объявляем сложные вложенные структуры 
  и хотим проверить статические ссылки
a
  println(1)
  2
  c() -> UInt
    3
    println(4)
    d
      5
    = 123

b = 10
e 
  20

# Проверяем результаты статических ссылок
res = 0

? a.1 != 2
  res = 1
? b != 10
  res = 2
? e != 20
  res = 3
? String(a) != "a"
  res = 4
? String(a.2) != "c"
  res = 5
? String(a.c) != "c"
  res = 6
? a.2() != 123
  res = 7
? a.c() != 123
  res = 8
? a.2.0 != 3
  res = 9
? a.c.0 != 3
  res = 10
? a.c.2 != 5
  res = 11
? a.c.2.0 != 5
  res = 12
? a.c.d != 5
  res = 13
? a.c.d.0 != 5
  res = 14

# Дополнительная проверка через новую ссылку на структуру
f = a.c.0

? f != 3
  res = 15

m = a

? String(m) != "a"
  res = 16

# Если не было ошибок при проверке, то всё правильно
exit(res)